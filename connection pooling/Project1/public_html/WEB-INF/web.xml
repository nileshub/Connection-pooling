<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
<servlet>
    <servlet-name>JDBCDataSourceExample</servlet-name>
    <servlet-class>project1.JDBCDataSourceExample</servlet-class> 
</servlet>

<servlet-mapping>
    <servlet-name>JDBCDataSourceExample</servlet-name>
    <url-pattern>/connectToPool</url-pattern>   
</servlet-mapping>
         
<servlet>
    <servlet-name>InsertData</servlet-name>
    <servlet-class>project1.InsertData</servlet-class> 
</servlet>

<servlet-mapping>
    <servlet-name>InsertData</servlet-name>
    <url-pattern>/Insert</url-pattern>   
</servlet-mapping>   
         
         
</web-app>
